<div class="span12">
  <h1>Order Up!</h1>
  <table class="table">
    <tr>
      <th>Order ID:</th>
      <th>User ID:</th>
      <th>Order Placed:</th>
      <th>Order Edited:</th>
      <th>Order Total</th>
    </tr>
      <td><%= order.id %></td>
      <td>John Doe</td>
      <td><%= order.created_at %></td>
      <td><%= order.updated_at %></td>
      <td><%= order.total_price %></td>
    </tr>
  </table>
</div>
<div class="span12">
  <table id="items">
    <tr>
      <th>Product</th>
      <th>Item Price</th>
      <th>Quantity</th>
      <th>Item Total</th>
    </tr>
    <% products.each do |product| %>
      <tr>
        <td><%= product.title %></td>
        <td><%= product.price %></td>
        <td><%= product.quantity %></td>
        <td><%= product.price * product.quantity %></td>
      </tr>
    <% end %>
  </table>
</div>