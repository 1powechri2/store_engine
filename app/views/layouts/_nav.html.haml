.navbar.navbar-fixed-top
  .navbar-inner
    .container
      %a.btn.btn-navbar{"data-target" => ".nav-collapse", "data-toggle" => "collapse"}
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a.brand{:href => "#"} Store Engine
      .nav-collapse
        %ul.nav
          %li.active
            %a{:href => root_url} Home
          %li
            - if current_user
              %a{:href => logout_path } Logout
            - else
              %a{:href => login_path } Login
          - if not current_user
            %li 
              %a{ :href => new_user_path } Sign Up
