#header
  %a{:href => "/"}
    %img#logo{:alt => "Living Social Logo", :src => "/assets/logo.png"}
%nav
  = link_to "Home", root_path
  - if current_user and current_user.admin
    %a{:href => dashboard_path} Dashboard
  - if current_user
    %a{:href => logout_path } Logout
  - else  
    %a{:href => login_path } Login
    %a{:href => new_user_path } Sign Up
    #search
      = render 'search/search', :value => ''