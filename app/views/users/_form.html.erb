<%= form_for @user do |f| %>
  <%= f.label :full_name %>
  <br />
  <%= f.text_field :full_name %>
  <br />
  <%= f.label :email_address %>
  <br />
  <%= f.text_field :email_address %>
  <br />
  <%= f.label :display_name %>
  <br />
  <%= f.text_field :display_name %>
  <br />
  <%= f.submit %>
<% end %>
